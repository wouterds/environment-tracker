{
  "presets": [
    "next/babel",
    "@zeit/next-typescript/babel",
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@resources": "./resources",
        "components": "./src/application/client/components",
        "application": "./src/application",
        "domain": "./src/domain",
        "infrastructure": "./src/infrastructure",
      },
    }],
  ],
}
