{
  "presets": [
    "next/babel",
    "@zeit/next-typescript/babel",
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@resources": "./resources",
        "components": "./src/components",
        "models": "./src/models",
        "repositories": "./src/repositories",
        "services": "./src/services",
        "styles": "./src/styles"
      },
    }],
  ],
}
