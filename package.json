    "lint": "./node_modules/.bin/tslint --format stylish 'src/**/*.ts' 'src/**/*.tsx'",
    "lint:fix": "./node_modules/.bin/tslint --format stylish --fix 'src/**/*.ts' 'src/**/*.tsx'"
  },
