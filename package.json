{
  "name": "environment-tracker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node -r ./src/bootstrap ./src/server",
    "dev": "next ./src",
    "sync:models": "ts-node -r ./src/bootstrap ./src/scripts/sync-models",
    "build": "npm run build:app && npm run build:server",
    "build:app": "NODE_ENV=production next build ./src",
    "build:server": "NODE_ENV=production tsc -p ./tsconfig.server.json",
    "lint": "npm run tslint && npm run stylelint",
    "lint:fix": "npm run tslint:fix && npm run stylelint:fix",
    "tslint": "tslint --format stylish './src/**/*.ts' './src/**/*.tsx'",
    "tslint:fix": "tslint --format stylish --fix './src/**/*.ts' './src/**/*.tsx'",
    "stylelint": "stylelint './src/**/*.ts'",
    "stylelint:fix": "stylelint --fix './src/**/*.ts'"
  },
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "mariadb": "^2.0.5",
    "next": "^8.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sequelize": "^5.9.4",
    "styled-components": "^4.3.2",
    "styled-components-breakpoint": "^2.1.1",
    "tsconfig-paths": "^3.8.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/next": "^8.0.5",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^4.1.16",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "dotenv-webpack": "^1.7.0",
    "next-compose-plugins": "^2.2.0",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3"
  }
}
