{
  "name": "env-tracker",
  "version": "0.1.0",
  "private": true,
  "author": "Wouter De Schuyter <wouter.de.schuyter@gmail.com>",
  "scripts": {
    "start": "npm run sync:db && npm run server:watch",
    "server": "./node_modules/.bin/ts-node ./src/server",
    "server:watch": "./node_modules/.bin/nodemon",
    "sync:db": "./node_modules/.bin/ts-node ./src/scripts/sync-models-database.ts",
    "lint": "./node_modules/.bin/tslint --format stylish 'src/**/*.ts'",
    "lint:fix": "./node_modules/.bin/tslint --format stylish --fix 'src/**/*.ts'"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "date-fns": "^1.30.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "mysql2": "^1.6.4",
    "sequelize": "^4.42.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/date-fns": "^2.6.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.15",
    "@types/sequelize": "^4.27.33",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2"
  }
}
