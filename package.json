    "lodash": "^4.17.11",
    "mysql2": "^1.6.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "recharts": "^1.4.2",
    "sequelize": "^4.42.0"
  },
  "devDependencies": {
