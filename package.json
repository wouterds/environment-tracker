    "@types/sequelize": "^4.27.33",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
